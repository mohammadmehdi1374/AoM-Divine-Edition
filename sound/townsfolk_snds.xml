<?xml version="1.0" encoding="UTF-8"?>

<protounitsounddef>
	<protounit name="Townsfolk">
		<soundtype name="Select">
			<variationlogic>
				<choice name="0">
					<soundset name="VillagerGreekSelect"></soundset>
				</choice>
				<choice name="1">
					<soundset name="VillagerGreekFemaleSelect"></soundset>
				</choice>
			</variationlogic>
		</soundtype>
		<soundtype name="Grunt">
			<variationlogic>
				<choice name="0">
					<soundset name="MaleGrunt"></soundset>
				</choice>
				<choice name="1">
					<soundset name="FemaleGrunt"></soundset>
				</choice>
			</variationlogic>
		</soundtype>
		<soundtype name="Death">
			<variationlogic>
				<choice name="0">
					<soundset name="MaleDeath"></soundset>
				</choice>
				<choice name="1">
					<soundset name="FemaleDeath"></soundset>
				</choice>
			</variationlogic>
		</soundtype>
		<soundtype name="Hit">
			<damagetypelogic>
				<choice name="Hack">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Slash">
					<soundset name="HackFlesh"></soundset>
				</choice>
				<choice name="Pierce">
					<soundset name="PierceFlesh"></soundset>
				</choice>
				<choice name="Crush">
					<soundset name="CrushFlesh"></soundset>
				</choice>
			</damagetypelogic>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="VillagerBirth"></soundset>
		</soundtype>
		<soundtype name="Acknowledge">
			<variationlogic>
				<choice name="0">
					<targetlogic>
						<choice name="default">
							<soundset name="VillagerGreekAcknowledge"></soundset>
						</choice>
						<choice name="enemy">
							<soundset name="VillagerGreekAttack"></soundset>
						</choice>
						<choice name="Berry Bush">
							<soundset name="VillagerGreekForage"></soundset>
						</choice>
						<choice name="Farm">
							<soundset name="VillagerGreekFarm"></soundset>
						</choice>
						<choice name="Tree">
							<soundset name="VillagerGreekLumberjack"></soundset>
						</choice>
						<choice name="Gold">
							<soundset name="VillagerGreekMine"></soundset>
						</choice>
						<choice name="Fish">
							<soundset name="VillagerGreekFish"></soundset>
						</choice>
						<choice name="Huntable">
							<soundset name="VillagerGreekHunt"></soundset>
						</choice>
						<choice name="Building">
							<soundset name="VillagerGreekRepair"></soundset>
						</choice>
					</targetlogic>
				</choice>
				<choice name="1">
					<targetlogic>
						<choice name="default">
							<soundset name="VillagerGreekFemaleAcknowledge"></soundset>
						</choice>
						<choice name="enemy">
							<soundset name="VillagerGreekFemaleAttack"></soundset>
						</choice>
						<choice name="Berry Bush">
							<soundset name="VillagerGreekFemaleForage"></soundset>
						</choice>
						<choice name="Farm">
							<soundset name="VillagerGreekFemaleFarm"></soundset>
						</choice>
						<choice name="Tree">
							<soundset name="VillagerGreekFemaleLumberjack"></soundset>
						</choice>
						<choice name="Gold">
							<soundset name="VillagerGreekFemaleMine"></soundset>
						</choice>
						<choice name="Fish">
							<soundset name="VillagerGreekFemaleFish"></soundset>
						</choice>
						<choice name="Huntable">
							<soundset name="VillagerGreekFemaleHunt"></soundset>
						</choice>
						<choice name="Building">
							<soundset name="VillagerGreekFemaleRepair"></soundset>
						</choice>
					</targetlogic>
				</choice>
			</variationlogic>
		</soundtype>
	</protounit>
</protounitsounddef>
